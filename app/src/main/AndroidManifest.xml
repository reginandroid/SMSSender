<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="app.com.regiko.smssender">
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SMSSender">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ReciverData"
            android:label="@string/title_activity_reciver_data"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity
            android:name=".ContactList"
            android:label="ContactList"
           >
        </activity>
        <activity
            android:name=".NewPhoneNumber"
            android:label="NewPhoneNumber"
            >
        </activity>
    </application>

</manifest>